{
  "name": "timelang",
  "version": "0.4.1",
  "description": "Parse natural language time expressions into structured data",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run compile:grammar && tsup",
    "compile:grammar": "nearleyc src/grammar.ne -o src/grammar.ts && { echo '// @ts-nocheck'; cat src/grammar.ts; } > tmp && mv tmp src/grammar.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "date",
    "time",
    "parser",
    "natural-language",
    "nlp",
    "duration",
    "timespan",
    "range"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "moo": "^0.5.2",
    "nearley": "^2.20.1"
  },
  "devDependencies": {
    "@types/moo": "^0.5.9",
    "@types/nearley": "^2.11.5",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.9",
    "prettier": "^3.7.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
